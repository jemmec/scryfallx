{
  "manifest_version": 2,
  "name": "ScryfallX",
  "version": "0.1",
  "description": "Extension for the Scryfall ",
  "browser_action": {
    "default_title": "ScryfallX"
  },
  "permissions": [
    "https://*/*"
  ],
  "content_security_policy": "default-src 'self';",
  "content_scripts": [
    {
      "css": ["/static/css/main.css"],
      "matches": ["https://scryfall.com/*"],
      "js": ["/static/js/main.js"]
    }
  ]
}
